{
    "name": "catch/ss-embedable-gallery",
    "description": "Allows you to create galleries and embed them in the CMS wysiwyg editor",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "gallery"],
	"license": "Proprietary",
    "authors": [
        {
            "name": "Aaron Latham-Anderson",
            "email": "aaron@catch.co.nz"
        }
    ],
    "require": {
        "php"                                   : ">=7.1",
        "composer/installers"                   : ">=1.0",
        "silverstripe/framework"                : "^4.3",
        "silverstripe/cms"                      : "^4.3",
        "azt3k/abc-silverstripe"                : "*",
        "jonom/focuspoint"                      : ">=2.0",
        "undefinedoffset/sortablegridfield"     : "^2",
        "micschk/silverstripe-excludechildren"  : "*"
    },
    "replace": {"catch/ss-embedable-gallery": "*"},
    "autoload-dev": {
        "psr-4": {
            "CatchDesign\\EmbedableGallery\\": "code/"
        }
    },
    "extra": {
        "expose": [
            "assets"
        ]
    }

}
